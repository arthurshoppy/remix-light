{
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM"
    ],
    "target": "ES2020",
    "moduleResolution": "node",
    "jsx": "preserve",
    "baseUrl": "./",
    "strict": true,
    "skipLibCheck": true,
    "inlineSourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "error"
  }
}